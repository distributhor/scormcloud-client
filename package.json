{
  "name": "scormcloud-client",
  "version": "0.0.4",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -b --clean && tsc -b",
    "lint": "eslint '{src,test}/**/*.{js,ts}'",
    "lint:fix": "eslint --fix '{src,test}/**/*.{js,ts}'",
    "lint:format": "prettier --write '{src,test}/**/*.{js,ts,json}'",
    "integration:test": "jest -c test/jest.integration.config.js",
    "test": "jest -c test/jest.unit.config.js --passWithNoTests"
  },
  "files": [
    "/dist"
  ],
  "dependencies": {
    "luxon": "^1.25.0",
    "superagent": "^6.1.0",
    "uuid": "^8.1.0",
    "yarn": ">=1.22.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.10",
    "@types/superagent": "^4.1.10",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.5.3",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push-with-test": "yarn lint && yarn test",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "*.{js,json,md,ts,tsx,jsx,vue}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  }
}
